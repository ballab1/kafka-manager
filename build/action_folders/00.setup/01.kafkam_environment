#!/bin/bash


declare -ar env_kafkam=(
    'KM_ARGS="${KM_ARGS:-"-Djava.net.preferIPv4Stack=true"}"'
    'KM_CONTEXT="${KM_CONTEXT:-/}"'
    'KM_VERSION="${KM_VERSION:-1.3.3.17}"'
    'KM_HOME="${KM_HOME:-/usr/local/kafka-manager}"'
    'KM_CONFIGFILE="${KM_HOME}/conf/application.conf"'
)

lib.updateRuntimeEnvironment "${env_kafkam[*]}" 
